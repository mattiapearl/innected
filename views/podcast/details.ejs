                <div class="episode-content">
                  <div class="episode-description">
                    <h1><%= podcast.title %></h1>
                    <% podcast.description.forEach((paragraph)=>{ %> 
                    <p class="text-description"><%= paragraph %></p>
                    <% }) %> 
                  </div>
                  <div class="additional-content">
                    <% if(podcast.links.length){ %>
                      <div class="episode-links">
                        <h5>RELATED LINKS</h5>
                       <ul>
                        <% podcast.links.forEach((link)=>{ %>
                        <% const nr = podcast.links.indexOf(link) %>  
                        <a href="<%= link %>"><%= podcast.linkNames[nr] %>  &rarr;</a>
                        <% }) %> 
                       </ul>  
                      </div>
                      <% } %> 
                    <div class="more-streams">
                      <h5>OTHER PLATFORMS</h5>
                     <ul>
                      <a href="<%= linkToEp %>">Youtube &rarr;</a>
                      <% if(podcast.spotiLink) {%> 
                        <a href="<%= podcast.spotiLink %> " target="_blank">Spotify &rarr;</a>
                        <%} %> 
                      <% if(podcast.appleLink) {%> 
                        <a href="<%= podcast.appleLink %> " target="_blank">Apple Podcast &rarr;</a>
                        <%} %> 
                     </ul>  
                    </div>
                    <div class="guests">
                        <% podcast.pRoles.forEach(element => { %> 
                          <h5><%= element %> </h5>
                          <% const nr = podcast.pRoles.indexOf(element);%>
                     <ul>
                       <% if(podcast.participants[nr].includes(",")){  %> 
                        <% const partecipanti = podcast.participants[nr].split(","); %>
                        <% partecipanti.forEach((name)=>{ %>
                          <li ><%= name %> </a>
                        <% }) %>  
                        <% }else{ %>
                      <li ><%= podcast.participants[nr] %> </a>
                     </ul>  
                        <% }}); %>  
                    </div>
                  </div>
