            <!-- LEFT SIDE -->
            <div class="podcast-nav">
              <a class="back mobile-hide" href="..">&larr; Go back</a>
              <ul class="episode-list hide-sb">
                <% list.forEach(element => { %> 
                  <% if(element.link == podcast.seoHandle){ %> 
                    <a href="#" class="selected"><%= element.tabName %> </a>
                    <% }else{ %>  
                  <a href="https://innectedpodcast.com/podcast/<%= element.link %>"><%= element.tabName %> </a>
                  <% }}); %> 
              </ul>
            </div>
            <!-- RIGHT SIDE -->
